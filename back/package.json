{
  "name": "back",
  "version": "1.0.0",
  "description": "wantodo backend",
  "apidoc": {
    "name": "Wantodo",
    "version": "1.0.0",
    "title": "Wantodo API",
    "description": "Wantodo API documentation",
    "url": "http://localhost:9090",
    "sampleUrl": "http://localhost:9090",
    "template": {
      "forceLanguage": "en",
      "withCompare": true,
      "withGenerator": false,
      "aloneDisplay": false
    },
    "order": [
      "Users",
      "Friends",
      "Pools",
      "Blind",
      "PostProFile",
      "InterestProfile",
      "PostFeed",
      "PostComment",
      "PostReComment",
      "InterestFeed",
      "InterestComment",
      "InterestRecomment",
      "InterestKeyword",
      "Notification",
      "Manage",
      "Upload",
      "SMS"
    ]
  },
  "main": "app.ts",
  "scripts": {
    "local": "npm run apidoc-generate:local && cross-env NODE_ENV=local nodemon app.ts",
    "dev": "cross-env NODE_ENV=develop nodemon app.ts",
    "prod": "cross-env NODE_ENV=production nodemon app.ts",
    "apidoc-generate:local": "apidoc -i src/ -o ./docs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apidoc": "^0.27.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.10.0",
    "google-auth-library": "^7.0.4",
    "mongoose": "^5.12.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/mongoose": "^5.10.4",
    "@types/uuid": "^8.3.0",
    "concurrently": "^6.0.1",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
